#include "main.h"
/**
 * string_toupper - changes string to uppercase
 * @c: string to be changed 
 * Return: returns c
 */
char *string_toupper(char *c)
{
  int i, j;
  i = 0;
  while (c[i] != '\0')
    {
      i++;
    }
  for (j = 0; j < i; j++)
    {
      if (c[j] >= 'a' && c[j] <= 'z')
	{
	  c[j] = c[j] - 32;

	}
    }
 return (c);
}

      
