#include "main.h"
/**
 * _strspn - sees how far the the two strings are equal
 * @s: this is the main string
 * @accept: these are the letters to be excluded
 * Return: returns the number of bytes 
 */
unsigned int _strspn(char *s, char *accept)
{
  int n;
  n = 1;

  while (s[n] == accept)
    {
      n++;
    }
  return (n);
}
