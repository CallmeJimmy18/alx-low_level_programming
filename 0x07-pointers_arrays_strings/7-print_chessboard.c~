#include "main.h"
#include <stdio.h>
/**
 * print_chessboard - this will print the chessboard
 * @a: this is the array thT contains the chess board
 */
void print_chessboard(char (*a)[8])
{
  int i;
  int j;

  for (i = 0; i < 8; i++)
    {
      for (j = 0; j < 8; j++)
	{
	  printf("%c", a[i][j]);
	}
      printf("\n");
    }
}
